{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Comment-Remover",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": [
        "activeTab", "scripting"
    ]
}
